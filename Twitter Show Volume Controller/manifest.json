{
    "manifest_version": 3,
    "name": "Twitter Show Volume Controller",
    "version": "0.1",
    "description": "force a controller on a twitter video to show up",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "content_scripts": [
        {
          "matches": ["*://twitter.com/*"],
          "js": ["scripts/content.js"]
        }
      ]
}